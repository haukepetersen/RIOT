[Unit]
Description=Deich Border Router Setup Service
Wants=network-online.target
After=network-online.target
After=time-sync.target
StopWhenUnneeded=yes

[Service]
ExecStart=/home/hauke/services/deichbr/deichbr.sh up
ExecStop=/home/hauke/services/deichbr/deichbr.sh down

[Install]
#WantedBy=multi-user.target
WantedBy=sys-devices-platform-soc-3f980000.usb-usb1-1\x2d1-1\x2d1.5-1\x2d1.5:1.2-net-usb0.device


